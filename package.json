{
  "name": "fis3-plugins",
  "private": true,
  "version": "1.0.0",
  "description": "plugins for fi3",
  "keywords": [
    "fis",
    "fis3"
  ],
  "main": "index.js",
  "repository": "https://github.com/fisker/fis3-plugins",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fisker/fis3-plugins/issues"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": []
  },
  "scripts": {
    "build": "node -r esm \"./scripts/build-packages.js\"",
    "bump-versions": "node -r esm \"./scripts/bump-versions.js\"",
    "clean": "rimraf packages",
    "dev": "rollup --config --watch",
    "dist": "lerna publish from-package --create-release github --no-changelog --amend --no-git-tag-version",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "postpublish": "yarn bump-versions && yarn build && git add versions.json && git add packages && git commit -m \"chore: bump versions [skip ci]\" --no-verify && git push",
    "release": "run-s clean lint build format dist"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@commitlint/cli": "8.2.0",
    "@fisker/commitlint-config": "1.1.3",
    "@fisker/eslint-config": "1.3.3",
    "@fisker/husky-config": "1.1.3",
    "@fisker/lint-staged-config": "1.0.5",
    "@fisker/prettier-config": "1.0.21",
    "babel-plugin-transform-async-to-promises": "0.8.14",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.5.1",
    "esm": "3.2.25",
    "fast-json-stable-stringify": "2.0.0",
    "husky": "3.0.7",
    "latest-version": "5.1.0",
    "lerna": "3.16.4",
    "lint-staged": "9.4.1",
    "make-dir": "3.0.0",
    "markdownlint-cli": "0.18.0",
    "mkdirp": "0.5.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "rollup": "1.22.0",
    "rollup-plugin-babel": "4.3.3",
    "write-prettier-file": "1.2.0"
  },
  "dependencies": {
    "@starptech/prettyhtml": "0.10.0",
    "autoprefixer": "9.6.1",
    "body-parser": "1.19.0",
    "browser-sync": "2.26.7",
    "clean-css": "4.2.1",
    "cssgrace": "3.0.0",
    "cssgrace-lite": "1.0.1",
    "ejs": "2.7.1",
    "eslint": "6.5.1",
    "execa": "2.0.4",
    "htmlhint": "0.11.0",
    "js-beautify": "1.10.2",
    "lodash": "4.17.15",
    "lodash.merge": "4.6.2",
    "morgan": "1.9.1",
    "node-sass": "4.12.0",
    "postcss": "7.0.18",
    "postcss-load-config": "2.1.0",
    "posthtml": "0.11.6",
    "posthtml-beautify": "0.3.0",
    "prettier": "1.18.2",
    "promise-synchronizer": "1.1.3",
    "pug": "2.0.4",
    "sass": "1.22.12",
    "serve-directory": "3.1.2",
    "serve-directory-theme-octicons": "1.3.0",
    "standard": "14.3.1",
    "stylelint": "11.0.0",
    "terser": "4.3.4",
    "uglify-js": "3.6.0",
    "yargs": "14.0.0",
    "yog-devtools": "0.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "homepage": "https://github.com/fisker/fis3-plugins#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "sideEffects": false
}
