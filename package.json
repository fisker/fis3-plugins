{
  "name": "fis3-plugins",
  "private": true,
  "version": "1.0.0",
  "description": "plugins for fi3",
  "keywords": [
    "fis",
    "fis3"
  ],
  "main": "index.js",
  "repository": "https://github.com/fisker/fis3-plugins",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fisker/fis3-plugins/issues"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": []
  },
  "scripts": {
    "build": "node -r esm \"./scripts/build-packages.js\"",
    "bump-versions": "node -r esm \"./scripts/bump-versions.js\"",
    "clean": "rimraf packages",
    "dev": "rollup --config --watch",
    "dist": "lerna publish",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint **/*.{js,mjs,vue}",
    "lint:markdown": "markdownlint **/*.md --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "postpack": "yarn bump-versions && git add versions.json && git commit -m \"chore: update versions cache\" && git push",
    "release": "run-s clean lint build format dist"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.4.5",
    "@commitlint/cli": "^8.0.0",
    "@fisker/commitlint-config": "^1.1.0",
    "@fisker/eslint-config": "^1.2.5",
    "@fisker/husky-config": "^1.1.0",
    "@fisker/lint-staged-config": "^1.0.4",
    "@fisker/prettier-config": "^1.0.17",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^6.0.1",
    "esm": "^3.2.25",
    "fast-json-stable-stringify": "^2.0.0",
    "husky": "^3.0.0",
    "latest-version": "^5.1.0",
    "lerna": "^3.15.0",
    "lint-staged": "^9.0.2",
    "make-dir": "^3.0.0",
    "markdownlint-cli": "^0.17.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.16.5",
    "rollup-plugin-babel": "^4.3.3"
  },
  "dependencies": {
    "@starptech/prettyhtml": "^0.10.0",
    "autoprefixer": "^9.6.0",
    "body-parser": "^1.19.0",
    "browser-sync": "^2.26.7",
    "clean-css": "^4.2.1",
    "cssgrace": "^3.0.0",
    "cssgrace-lite": "^1.0.1",
    "ejs": "^2.6.2",
    "eslint": "^6.0.1",
    "execa": "^2.0.2",
    "htmlhint": "^0.11.0",
    "js-beautify": "^1.10.0",
    "lodash": "^4.17.11",
    "lodash.merge": "^4.6.1",
    "morgan": "^1.9.1",
    "node-sass": "^4.12.0",
    "postcss": "^7.0.17",
    "postcss-load-config": "^2.1.0",
    "posthtml": "^0.11.4",
    "posthtml-beautify": "^0.2.6",
    "prettier": "^1.18.2",
    "promise-synchronizer": "^1.1.1",
    "pug": "^2.0.4",
    "sass": "^1.22.3",
    "serve-directory": "^3.1.1",
    "serve-directory-theme-octicons": "^1.3.0",
    "standard": "^12.0.1",
    "stylelint": "^10.1.0",
    "terser": "^4.0.2",
    "uglify-js": "^3.6.0",
    "yargs": "^13.2.4",
    "yog-devtools": "^0.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "homepage": "https://github.com/fisker/fis3-plugins#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "sideEffects": false
}
