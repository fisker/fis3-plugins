{
  "name": "fis3-plugins",
  "version": "1.0.0",
  "private": true,
  "description": "plugins for fi3",
  "keywords": [
    "fis",
    "fis3"
  ],
  "homepage": "https://github.com/fisker/fis3-plugins#readme",
  "bugs": {
    "url": "https://github.com/fisker/fis3-plugins/issues"
  },
  "repository": "fisker/fis3-plugins",
  "funding": "https://github.com/fisker/fis3-plugins?sponsor=1",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false,
  "main": "index.js",
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": []
  },
  "scripts": {
    "build": "node \"./scripts/build-packages.mjs\"",
    "bump-versions-cache": "node \"./scripts/bump-versions-cache.mjs\"",
    "clean": "run-p clean:*",
    "clean:dist": "del-cli packages",
    "commit-versions-cache": "git add versions.json && git add packages && git commit -m \"chore: bump versions cache [skip ci]\" --no-verify && git push",
    "dist": "lerna publish",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s clean lint build format dist bump-versions-cache commit-versions-cache"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@starptech/prettyhtml": "0.10.0",
    "autoprefixer": "10.4.2",
    "body-parser": "1.19.2",
    "browser-sync": "2.27.7",
    "clean-css": "5.2.4",
    "cssgrace": "3.0.0",
    "cssgrace-lite": "1.0.1",
    "ejs": "3.1.6",
    "eslint": "8.10.0",
    "execa": "5.1.1",
    "fast-cartesian-product": "2.0.1",
    "htmlhint": "0.16.3",
    "js-beautify": "1.14.0",
    "lodash": "4.17.21",
    "lodash.merge": "4.6.2",
    "morgan": "1.10.0",
    "postcss": "8.4.8",
    "postcss-load-config": "3.1.3",
    "posthtml": "0.16.6",
    "posthtml-beautify": "0.7.0",
    "prettier": "2.5.1",
    "promise-synchronizer": "3.0.0",
    "pug": "3.0.2",
    "sass": "1.45.2",
    "serve-directory": "4.0.0",
    "serve-directory-theme-octicons": "3.0.0",
    "standard": "16.0.4",
    "stylelint": "14.5.3",
    "terser": "5.10.0",
    "uglify-js": "3.14.5",
    "yargs": "16.2.0",
    "yog-devtools": "0.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/preset-env": "7.16.11",
    "@commitlint/cli": "16.2.1",
    "@fisker/commitlint-config": "2.0.2",
    "@fisker/eslint-config": "10.0.13",
    "@fisker/husky-config": "4.1.2",
    "@fisker/lint-staged-config": "3.1.4",
    "@fisker/prettier-config": "5.0.5",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-commonjs": "21.0.2",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.1.3",
    "babel-plugin-transform-async-to-promises": "0.8.18",
    "core-js": "3.21.1",
    "cz-conventional-changelog": "3.3.0",
    "del-cli": "4.0.1",
    "eslint": "8.10.0",
    "esm-utils": "3.0.0",
    "husky": "7.0.4",
    "latest-version": "6.0.0",
    "lerna": "4.0.0",
    "lint-staged": "12.3.5",
    "listr": "0.14.3",
    "make-dir": "3.1.0",
    "markdownlint-cli": "0.31.1",
    "mkdirp": "1.0.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "rollup": "2.69.2",
    "rollup-plugin-prettier": "2.2.2",
    "sort-object-keys": "1.1.3",
    "sort-package-json": "1.54.0",
    "write-prettier-file": "2.1.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
